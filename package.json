{
  "name": "front-irix",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "c": "ng g c components/cita/list-cita/itemListCita --skip-import",
    "d": "ng generate directive directives/dynamicTag --skip-import",
    "m": "ng g m modules/citas/citas -m=app --route citas --flat",
    "s": "ng g s services/ServiciosApi/serviciosApi",
    "dev": "ng s -o",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run front-irix:serve-ssr",
    "serve:ssr": "node dist/front-irix/server/main.js",
    "build:ssr": "ng build && ng run front-irix:server",
    "prerender": "ng run front-irix:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/common": "^15.2.10",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/platform-server": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@nguniversal/express-engine": "^15.2.1",
    "@types/pica": "^9.0.5",
    "express": "^4.15.2",
    "file-saver": "^2.0.5",
    "pica": "^9.0.1",
    "rxjs": "~7.8.0",
    "socket.io-client": "^4.7.5",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.11",
    "@angular/cli": "~15.2.11",
    "@angular/compiler-cli": "^15.2.0",
    "@nguniversal/builders": "^15.2.1",
    "@types/express": "^4.17.0",
    "@types/file-saver": "^2.0.7",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^14.15.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.9.4"
  }
